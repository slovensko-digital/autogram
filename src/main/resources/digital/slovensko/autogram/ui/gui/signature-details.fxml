<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>
<?import javafx.scene.text.TextFlow?>
<?import javafx.scene.web.WebView?>
<VBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      prefHeight="800"
      maxHeight="800"
      minWidth="768"
      fx:id="mainBox">

    <TextFlow><Text styleClass="autogram-heading-m" text="%signature.details.title"/></TextFlow>

    <!-- HTML visualization of validation report -->
    <VBox fx:id="webViewContainer" managed="false" visible="false" VBox.vgrow="ALWAYS">
        <WebView fx:id="webView" focusTraversable="false" VBox.vgrow="ALWAYS"/>
    </VBox>

</VBox>
