					<xs:documentation>Heading 3 (Tertiary Heading)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element maxOccurs="unbounded" minOccurs="0" name="big"
				type="FormattedTextType">
				<xs:annotation>
					<xs:documentation>Bigger font</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element maxOccurs="unbounded" minOccurs="0" name="small"
				type="FormattedTextType">
				<xs:annotation>
					<xs:documentation>Bigger font</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element maxOccurs="unbounded" minOccurs="0" name="br">
				<xs:annotation>
					<xs:documentation>Line break</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element maxOccurs="unbounded" minOccurs="0" name="a"
				type="LinkType">
				<xs:annotation>
					<xs:documentation>Link</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element maxOccurs="unbounded" minOccurs="0" name="editable"
				type="FormattedTextType">
				<xs:annotation>
					<xs:documentation>Containing text is allowed to be edited later by
						user. Usually translates to a SPAN tag in output html with
						"editable" class.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element maxOccurs="unbounded" minOccurs="0" name="blockquote"
				type="BlockquoteType">
				<xs:annotation>
					<xs:documentation>Block quote</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:group ref="TemplateTextGroup" />
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="TableType">
		<xs:sequence>
			<xs:element minOccurs="0" name="caption" type="FormattedTextType" />
			<xs:element minOccurs="0" name="thead" type="TableHeaderType" />
			<xs:choice maxOccurs="unbounded" minOccurs="0">
				<xs:element name="tr" type="TableRowType" />
				<xs:group ref="TemplateRowGroup" />
			</xs:choice>
		</xs:sequence>
		<xs:attribute name="width" type="xs:positiveInteger" use="optional" />
		<xs:attribute default="center" name="align" type="FormattedAlignType"
			use="optional" />
		<xs:attribute name="border" use="optional">
			<xs:simpleType>
				<xs:restriction base="xs:nonNegativeInteger">
					<xs:enumeration value="0" />
					<xs:enumeration value="1" />
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="TableHeaderType">
		<xs:sequence>
			<xs:element maxOccurs="unbounded" name="tr"
				type="TableHeaderRowType" />
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="TableHeaderRowType">
		<xs:choice maxOccurs="unbounded" minOccurs="0">
			<xs:element name="th" type="TableCellType" />
			<xs:group ref="TemplateHeaderCellGroup" />
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="TableRowType">
		<xs:choice maxOccurs="unbounded" minOccurs="0">
			<xs:element name="td" type="TableCellType" />
			<xs:group ref="TemplateCellGroup" />
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="TableCellType">
		<xs:complexContent>
			<xs:extension base="FormattedTextType">
				<xs:attribute name="align" type="FormattedAlignType"
					use="optional" />
				<xs:attribute default="1" name="colspan" type="xs:positiveInteger"
					use="optional" />
				<xs:attribute default="1" name="rowspan" type="xs:positiveInteger"
					use="optional" />
				<xs:attribute name="width" type="xs:positiveInteger"
					use="optional" />
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="FormattedParagraphType">
		<xs:complexContent>
			<xs:extension base="FormattedTextType">
				<xs:attribute name="align" type="FormattedAlignType"
					use="optional" />
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="FormattedHeadingType">
		<xs:simpleContent>
			<xs:extension base="xs:string">
				<xs:attribute name="align" type="FormattedAlignType"
					use="optional" />
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:simpleType name="FormattedAlignType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="left" />
			<xs:enumeration value="center" />
			<xs:enumeration value="right" />
			<xs:enumeration value="justify" />
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType mixed="true" name="FormattedUnorderedListType">
		<xs:choice maxOccurs="unbounded" minOccurs="0">
			<xs:element name="li" type="FormattedTextType">
				<xs:annotation>
					<xs:documentation>List item of an (un)ordered list
					</xs:documentation>
					<xs:documentation>Note that it has to be allowed on this top level,
						otherwise we couldn't sorround it with other element (e.g. "rs")
						if needed</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:group ref="TemplateListGroup" />
		</xs:choice>
	</xs:complexType>
	<xs:complexType mixed="true" name="FormattedOrderedListType">
		<xs:complexContent mixed="true">
			<xs:extension base="FormattedUnorderedListType">
				<xs:attribute name="type" type="FormattedListTypes" />
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:simpleType name="FormattedListTypes">
		<xs:restriction base="xs:string">
			<xs:enumeration value="1" />
			<xs:enumeration value="A" />
			<xs:enumeration value="a" />
			<xs:enumeration value="I" />
			<xs:enumeration value="i" />
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="LinkType">
		<xs:simpleContent>
			<xs:extension base="xs:string">
				<xs:attribute name="href" type="xs:string" use="required" />
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="BlockquoteType">
		<xs:complexContent>
			<xs:extension base="FormattedTextType">
				<xs:attribute name="style" type="xs:string" use="optional" />
				<xs:attribute name="dir" type="xs:string" use="optional" />
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
</xs:schema>