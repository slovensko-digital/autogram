<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xmlns="http://schemas.gov.sk/form/36126624.PovolenieZdravotnictvo.sk/1.9"
	attributeFormDefault="unqualified" elementFormDefault="qualified"
	targetNamespace="http://schemas.gov.sk/form/36126624.PovolenieZdravotnictvo.sk/1.9"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<xs:element name="OutputDocument">
		<xs:annotation>
			<xs:documentation>Výstupný elektronický formulár pre Povolenie na
				prevádzkovanie zdravotníckeho zariadenia alebo poskytovanie
				lekárenskej starostlivosti (rozhodnutie, oznámenie)
			</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="RequestBase">
					<xs:sequence>
						<xs:element maxOccurs="1" minOccurs="1" name="IssuedBy"
							type="POPersonDataCType" />
						<xs:element maxOccurs="1" minOccurs="1" name="IssuedTo"
							type="EHPersonDataCType" />
						<xs:element maxOccurs="unbounded" minOccurs="0"
							name="ZdravotnickeZariadenie" nillable="true"
							type="ZdravotnickeZariadenieCType" />
						<xs:element maxOccurs="unbounded" minOccurs="0"
							name="OdbornyZastupca" nillable="true" type="OdbornyZastupcaCType" />
						<xs:element maxOccurs="1" minOccurs="1" name="Rozhodnutie"
							type="RozhodnutieCType" />
					</xs:sequence>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="DeliveryNameAddressCType">
		<xs:sequence>
			<xs:element name="FormattedName" type="String1023" />
			<xs:element name="FormattedAddress" type="String1023" />
			<xs:element name="RecipientID" type="String1023"
				minOccurs="0" />
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="DovozMimoEUCType">
		<xs:sequence>
			<xs:element name="Country" type="CodelistDataElementCType" />
			<xs:element name="CorporateBodyFullName" type="CorporateBodyFullNameSCType" />
			<xs:element name="AddressLine" type="AddressLineType" />
			<xs:element name="TypTkaniva" type="FormattedTextType" />
			<xs:element maxOccurs="unbounded" minOccurs="0"
				name="InternaCinnost" nillable="true" type="CodelistDataElementCType" />
			<xs:element maxOccurs="unbounded" minOccurs="0"
				name="ExternaCinnost" nillable="true" type="CodelistDataElementCType" />
			<xs:element name="OpisPohybuTkaniva" type="FormattedTextType" />
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="TkanivoveZariadenieCType">
		<xs:sequence>
			<xs:element name="EUKod" type="String100" />
			<xs:element minOccurs="0" name="DruhCinnosti" type="CodelistDataElementCType" />
			<xs:element name="TypTkaniva" type="FormattedTextType" />
			<xs:element minOccurs="0" name="VyvozTypTkaniva" type="FormattedTextType" />
			<xs:element minOccurs="0" name="DovozEUTypTkaniva" type="FormattedTextType" />
			<xs:element maxOccurs="unbounded" minOccurs="0" name="DovozMimoEU"
				nillable="true" type="DovozMimoEUCType" />
			<xs:element name="OdberTkaniv" type="CorporateBodyFullNameSCType"
				minOccurs="0" maxOccurs="unbounded" />
			<xs:element name="TestovanieTkaniv" type="CorporateBodyFullNameSCType"
				maxOccurs="unbounded" minOccurs="0" />
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="APSCType">
		<xs:sequence>
			<xs:element name="PevnyBod" type="String1023" />
			<xs:element name="SpadoveUzemie" type="String1023" />
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="OdbornyUtvarCType">
		<xs:sequence>
			<xs:element name="DruhOU" type="CodelistDataElementCType" />
			<xs:element name="DruhOddelenia" type="CodelistDataElementCType" />
			<xs:element name="Nazov" type="String1023" />
			<xs:element maxOccurs="unbounded" minOccurs="0"
				name="OdborneZameranie" nillable="true" type="CodelistDataElementCType" />
			<xs:element name="PocetLozok" type="xs:integer" />
			<xs:element name="JIS" type="xs:boolean" />
			<xs:element name="JISPocetLozok" type="xs:integer" />
			<xs:element name="PrijmovaAmbulancia" type="xs:boolean" />
			<xs:element name="UstavnaPohotovostnaSluzba" type="xs:boolean" />
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ZZariadenieCType">
		<xs:sequence>
			<xs:element maxOccurs="1" minOccurs="1" name="DruhZariadenia"
				type="CodelistDataElementCType" />
			<xs:element maxOccurs="unbounded" minOccurs="0"
				name="PhysicalAddress" type="PhysicalAddressCType">
				<xs:annotation>
					<xs:documentation>Adresa prevadzkarne</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element maxOccurs="unbounded" minOccurs="0"
				name="OdborneZameranie" nillable="true" type="CodelistDataElementCType" />
   <xs:element maxOccurs="unbounded" minOccurs="0" name="Ups" nillable="true"
               type="CodelistDataElementCType"/>
   <xs:element maxOccurs="unbounded" minOccurs="0" name="ID"
				type="IDCType">
				<xs:annotation>
					<xs:documentation>D.1.6 Zložený dátový prvok pre rôzne
						identifikátory.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<!-- TkanivoveZariadenie nepovinny 0..1 -->
			<xs:element maxOccurs="1" minOccurs="0" name="TkanivoveZariadenie"
				nillable="true" type="TkanivoveZariadenieCType" />
			<!-- OdborneUtvary nepovinny 0..N -->
			<xs:element maxOccurs="unbounded" minOccurs="0" name="OdbornyUtvar"
				nillable="true" type="OdbornyUtvarCType" />
			<xs:element minOccurs="0" name="DatumZaciatkuCinnosti"
				nillable="true" type="xs:date" />
			<xs:element minOccurs="0" name="DatumUkonceniaCinnosti"
				nillable="true" type="xs:date" />
			<xs:element minOccurs="0" name="DatumPozastaveniaOd"
				nillable="true" type="xs:date" />
			<xs:element minOccurs="0" name="DatumPozastaveniaDo"
				nillable="true" type="xs:date" />
			<xs:element maxOccurs="1" minOccurs="0" name="NazovZariadenia"
				nillable="true" type="xs:string" />
			<xs:element maxOccurs="1" minOccurs="0" name="APS"
				nillable="true" type="APSCType" />
   <xs:element minOccurs="0" name="RozsahCinnosti" type="FormattedTextType"/>
   <xs:element name="SidloStaniceZZS" type="String1023" minOccurs="0"/>
   <xs:element name="OpravnenieNaPrepravuLetunom" type="String1023" minOccurs="0"/>
   <xs:element maxOccurs="1" minOccurs="0" name="IndikacneZariadenie" nillable="true" type="FormattedTextType"/>
  </xs:sequence>
	</xs:complexType>
	<xs:complexType name="ZdravotnickeZariadenieCType">
		<xs:complexContent>
			<xs:extension base="ZZariadenieCType">
				<xs:sequence>
					<xs:element maxOccurs="unbounded" minOccurs="0"
						name="PodradeneZZ" nillable="true" type="ZZariadenieCType" />
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="OdbornyZastupcaCType">
		<xs:sequence>
			<xs:element maxOccurs="1" minOccurs="0" name="PhysicalPerson"
				type="EHPhysicalPersonCType">
				<xs:annotation>
					<xs:documentation>Zlozeny datovy prvok pre popis fyzickej osoby
						rozsireny o atributy pre zdravotnickeho pracovnika
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element maxOccurs="unbounded" minOccurs="0"
				name="PhysicalAddress" type="PhysicalAddressCType">
				<xs:annotation>
					<xs:documentation>D.1.3 Zložený dátový prvok pre určenie adresy.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element maxOccurs="unbounded" minOccurs="0" name="IDZZ"
				type="OdbornyZastupcaIDZZCType">
				<xs:annotation>
					<xs:documentation>Zložený dátový prvok pre zoznam identifikatorov
						zdravotnickych zariadeni</xs:documentation>
				</xs:annotation>
			</xs:element>
   <xs:element maxOccurs="1" minOccurs="1" name="DruhOdbornehoZastupcu" type="CodelistDataElementCType"/>
  </xs:sequence>
	</xs:complexType>
        <xs:complexType name="OdbornyZastupcaIDZZCType">
		<xs:sequence>
			<xs:element minOccurs="0" name="IDZZ"
				type="IDCType">
				<xs:annotation>
					<xs:documentation>Zložený dátový prvok pre zoznam identifikatorov
						zdravotnickych zariadeni</xs:documentation>
				</xs:annotation>
			</xs:element>
             <xs:element name="FunkciaOd" type="xs:date" minOccurs="0"/>
             <xs:element name="FunkciaDo" type="xs:date" minOccurs="0"/>
            </xs:sequence>
	</xs:complexType>
	<xs:complexType name="RozhodnutieCType">
		<xs:annotation>
			<xs:documentation>Zlozeny datovy prvok pre rozne informacie o
				rozhodnuti</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="DatumVydaniaRozhodnutia" type="xs:date" />
			<xs:element minOccurs="0" name="DatumUcinnosti" type="xs:date" />
			<xs:element minOccurs="0" name="DatumNadobudnutiaPravoplatnosti"
				type="xs:date" />
			<xs:element minOccurs="0" name="PozastavenieDo" type="xs:date" />
   <xs:element minOccurs="0" name="DatumUcinnostiDo" type="xs:date"/>
   <xs:element name="CisloRozhodnutia" type="NonEmptyStringType" />
			<xs:element minOccurs="0" name="PredchCisloRozhodnutia"
				type="NonEmptyStringType" />
			<xs:element minOccurs="0" name="PredchDatumVydaniaRozhodnutia"
				type="xs:date" />
			<xs:element minOccurs="0"
				name="PredchDatumNadobudnutiaPravoplatnosti" type="xs:date" />
			<xs:element minOccurs="0" name="Subject" nillable="true"
				type="xs:string" />
			<xs:element minOccurs="0" name="UvodVyroku" type="FormattedTextType" />
			<xs:element minOccurs="0" name="ZaverVyroku" type="FormattedTextType" />
			<xs:element minOccurs="0" name="Odovodnenie" type="FormattedTextType" />
			<xs:element minOccurs="0" name="Poucenie" type="FormattedTextType" />
			<xs:element minOccurs="0" name="ResponsiblePerson"
				nillable="true" type="ResponsiblePersonType" />
			<xs:element minOccurs="0" name="DeliverTo" nillable="true"
				type="DeliveryNameAddressCType" />
			<xs:element minOccurs="0" name="CarbonCopy" nillable="true"
				type="DeliveryNameAddressCType" maxOccurs="unbounded" />
			<xs:element minOccurs="1" name="MiestoVydania" nillable="true"
				type="xs:string">
				<xs:annotation>
					<xs:documentation>Miesto vydania rozhodnutia, napríklad "V
						Bratislave"</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element minOccurs="0" name="TypPovolenia" nillable="true"
				type="xs:string">
				<xs:annotation>
					<xs:documentation>Typy povolení: 578 - zdravotnícke zariadenia, 362- lekárenská starostlivosť, 538 - kúpele, 355 - RUVZ</xs:documentation>
				</xs:annotation>
			</xs:element>
   <xs:element minOccurs="0" name="CinnostPovolenia" nillable="true" type="xs:string">
    <xs:annotation>
     <xs:documentation>Množina hodnôt:prípravu transfúznych liekov v zariadeníindividuálnu prípravu liekov na inovatívnu liečbu v zariadeníposkytovanie lekárenskej starostlivosti v nemocničnej lekárnivýrobu humánnych liekov v zariadenívýrobu skúšaných humánnych produktov a skúšaných humánnych liekov v zariadeníveľkodistribúciu humánnych liekov v zariadeníposkytovanie lekárenskej starostlivosti v zariadení</xs:documentation>
    </xs:annotation>
   </xs:element>
   <xs:element minOccurs="0" name="DruhDokladu" nillable="true"
				type="xs:string">
				<xs:annotation>
					<xs:documentation>Hodnoty: VYTVORENIE - vydanie prvého povolenia,
						NOVE_ROZHODNUTIE - zmena povolenia novým rozhodnutím,
						VYZNACENIE_ZMENY - vyznačenie zmeny v povolení, ZRUSENIE -
						zrušenie povolenia, POZASTAVENIE - dočasné pozastavenie povolenia,
						ZANIK - zánik povolenia</xs:documentation>
				</xs:annotation>
			</xs:element>
   <xs:element minOccurs="0" name="Spravoplatnit" nillable="true" type="xs:boolean">
    <xs:annotation>
     <xs:documentation>Hodnoty: VYTVORENIE - vydanie prvého povolenia,
						NOVE_ROZHODNUTIE - zmena povolenia novým rozhodnutím,
						VYZNACENIE_ZMENY - vyznačenie zmeny v povolení, ZRUSENIE -
						zrušenie povolenia, POZASTAVENIE - dočasné pozastavenie povolenia,
						ZANIK - zánik povolenia</xs:documentation>
    </xs:annotation>
   </xs:element>
  </xs:sequence>
	</xs:complexType>
	<xs:complexType name="ResponsiblePersonType">
		<xs:sequence>
			<xs:group ref="ProfilePhysicalPersonFormattedNameOnly" />
			<xs:element name="Role" type="NonEmptyStringType" />
		</xs:sequence>
	</xs:complexType>
	<xs:group name="ProfilePhysicalPersonFormattedNameOnly">
		<xs:sequence>
			<xs:element name="PersonData">
				<xs:complexType>
					<xs:sequence>
						<xs:group ref="PhysicalPerson_formattedName" />
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:group>
	<xs:group name="PhysicalPerson_formattedName">
		<xs:sequence>
			<xs:element name="PhysicalPerson">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="PersonName" type="FormattedNameType" />
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:group>
	<xs:complexType name="EHPhysicalPersonCType">
		<xs:complexContent>
			<xs:extension base="PhysicalPersonCType">
				<xs:sequence>
					<xs:element minOccurs="0"
						name="ZdravotnickePovolanie" type="CodelistDataElementCType" maxOccurs="1"/>
					<xs:element maxOccurs="unbounded" minOccurs="0"
						name="StudijnyOdbor" nillable="true" type="CodelistDataElementCType" />
					<xs:element maxOccurs="unbounded" minOccurs="0"
						name="SpecializacnyOdbor" nillable="true" type="CodelistDataElementCType" />
					<xs:element maxOccurs="1" minOccurs="0"
						name="IDPovolovaciehoOrganu" type="CodelistDataElementCType" />
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<!-- person data -->
	<xs:element name="EHPersonData" type="EHPersonDataCType">
		<xs:annotation>
			<xs:documentation>D.1. Zlozeny datovy prvok pre rozne informacie o
				osobe.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="POPersonDataCType">
		<xs:annotation>
			<xs:documentation>D.1 Zlozeny datovy prvok pre rozne informacie o
				osobe typu povolovaci organ</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element maxOccurs="1" minOccurs="0" name="CorporateBody"
				type="CorporateBodyCType">
				<xs:annotation>
					<xs:documentation>D.1.2 Zlozeny datovy prvok pre popis pravnickej
						osoby alebo subjektu, ktory nie je pravnickou osobou ani fyzickou
						osobou v zmysle pravneho poriadku Slovenskej republiky.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element minOccurs="1" name="PhysicalAddress" type="PhysicalAddressCType">
				<xs:annotation>
					<xs:documentation>D.1.3 Zložený dátový prvok pre určenie adresy.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element maxOccurs="unbounded" minOccurs="0" name="ID"
				type="IDCType">
				<xs:annotation>
					<xs:documentation>D.1.6 Zložený dátový prvok pre rôzne
						identifikátory.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element maxOccurs="1" minOccurs="1"
				name="IDPovolovaciehoOrganu" type="CodelistDataElementCType">
				<xs:annotation>
					<xs:documentation>Identifikator povolovacieho organu
					</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="EHPersonDataCType">
		<xs:annotation>
			<xs:documentation>D.1 Zlozeny datovy prvok pre rozne informacie o
				osobe</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:sequence minOccurs="1">
				<xs:element maxOccurs="1" minOccurs="0" name="PhysicalPerson"
					type="EHPhysicalPersonCType">
					<xs:annotation>
						<xs:documentation>Zlozeny datovy prvok pre popis fyzickej osoby
							rozsireny o atributy pre zdravotnickeho pracovnika
						</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element maxOccurs="1" minOccurs="0" name="CorporateBody"
					type="CorporateBodyCType">
					<xs:annotation>
						<xs:documentation>D.1.2 Zlozeny datovy prvok pre popis pravnickej
							osoby alebo subjektu, ktory nie je pravnickou osobou ani fyzickou
							osobou v zmysle pravneho poriadku Slovenskej republiky.
						</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
			<xs:element maxOccurs="unbounded" minOccurs="1"
				name="PhysicalAddress" type="PhysicalAddressCType">
				<xs:annotation>
					<xs:documentation>D.1.3 Zložený dátový prvok pre určenie adresy.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element maxOccurs="unbounded" minOccurs="0" name="ID"
				type="IDCType">
				<xs:annotation>
					<xs:documentation>D.1.6 Zložený dátový prvok pre rôzne
						identifikátory.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<!-- physical address -->
	<xs:element name="PhysicalAddress" type="PhysicalAddressCType">
		<xs:annotation>
			<xs:documentation>D.1.3 Zlozeny datovy prvok pre urcenie adresy.
			</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="PhysicalAddressCType">
		<xs:annotation>
			<xs:documentation>D.1.3 Zlozeny datovy prvok pre urcenie adresy.
			</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element minOccurs="0" name="AddressLine" type="AddressLineType">
				<xs:annotation>
					<xs:documentation>D.1.3.1 Formatovana informacia, ktora urcuje
						urcitu adresu (katastralne uzemie, nazov hospodarskej usadlosti a
						podobne).</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element minOccurs="0" name="Country" type="CodelistDataElementCType">
				<xs:annotation>
					<xs:documentation>D.1.3.2 Zlozeny datovy prvok pre zakladny
						ciselnik obsahujuci udaje o state. Podradene prvky sa vyplnaju v
						sulade so zakladnym ciselnikom CL000086 Krajina OSN.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element minOccurs="0" name="County" type="CodelistDataElementCType">
				<xs:annotation>
					<xs:documentation>D.1.3.4 Zlozeny datovy prvok pre zakladny
						ciselnik obsahujuci udaje o okrese ako uzemnospravnom cleneni. V
						podradenych prvkoch sa uvadza textova informacia podla pouziteho
						atributu zakladneho ciselnika CL000024 Lokalna statisticka uzemna
						jednotka 1 – okres.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element minOccurs="0" name="Municipality" type="CodelistDataElementCType">
				<xs:annotation>
					<xs:documentation>D.1.3.5 Zlozeny datovy prvok pre zakladny
						ciselnik obsahujuci udaje o obci. Obcou je uzemnospravna jednotka
						charakterizovana suvislym domovym osidlenim a vlastnym nazvom.
						Obcou je aj mesto. V podradenych prvkoch sa uvadza textova
						informacia podla pouziteho atributu zakladneho ciselnika CL000025
						Lokalna statisticka uzemna jednotka 2 – obec.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element minOccurs="0" name="StreetName" type="StreetNameType">
				<xs:annotation>
					<xs:documentation>D.1.3.7 Obsahuje nazov ulice. Ak obec nema nazov
						ulice, udava sa tu nazov obce.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element minOccurs="0" name="BuildingNumber" type="BuildingNumberType">
				<xs:annotation>
					<xs:documentation>D.1.3.8 Obsahuje nazov budovy alebo cislo domu.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element minOccurs="0" name="PropertyRegistrationNumber"
				type="PropertyRegistrationNumberType">
				<xs:annotation>
					<xs:documentation>D.1.3.9 Obsahuje supisne cislo budovy.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element minOccurs="0" name="AddressPoint" type="AddressPointCType">
				<xs:annotation>
					<xs:documentation>D.1.3.12 Zlozeny datovy prvok, ktory je priamym
						priestorovym identifikatorom adresy v suradniciach zavaznych
